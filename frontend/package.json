{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prepare": "husky install"
  },
  "dependencies": {
    "@radix-icons/vue": "^1.0.0",
    "@tailwindcss/vite": "^4.1.5",
    "axios": "^1.9.0",
    "d3": "^7.9.0",
    "lodash": "^4.17.21",
    "shadcn-vue": "^2.1.0",
    "vue": "^3.5.13",
    "vue-router": "^4.2.5",
    "pinia": "^2.1.7"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/vue": "^8.0.2",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/test-utils": "^2.4.4",
    "@vitest/coverage-v8": "^1.2.2",
    "@vitest/ui": "^1.2.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.21.1",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.5.3",
    "prettier": "^3.2.5",
    "tailwindcss": "^4.1.5",
    "vite": "^6.3.5",
    "vitest": "^1.2.2"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
